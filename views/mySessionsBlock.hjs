{{#sessions}}

<div id="session-{{id}}">
    <div class="row">
        <div class="col-md-2 sessionEnroll">
            <p style="font-size:80%">Enroll to this session</p>
            <div class="switch">
                <input id="toggle-{{id}}" class="cmn-toggle cmn-toggle-round" type="checkbox" name="{{parentTab}}" value="{{id}}" data-session="{{id}}"/>
                <label for="toggle-{{id}}"></label>
            </div>
        </div>
        <div class="col-md-8 text-left">
            <h3>{{{title}}} </h3>
            <h6>{{speakerName}} / {{speakerProfession}}, {{speakerCompany}}</h6>
            <p>{{{content}}}</p>
        </div>
    </div>
</div>

{{/sessions}} {{/hasChildren}} {{^hasChildren}}

